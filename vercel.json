{
  "buildCommand": "npm run build",
  "outputDirectory": "out",
  "framework": null,
  "cleanUrls": false,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(prototipo-produccion-rapida|desarrollo-contextualizado-estandarizado|orquestacion-auditoria-agentes|desarrollo-tdd|gestion-requisitos-flujo-trabajo-mcp|investigacion-aprendizaje-acelerado-referencias|dashboard-analitico-empresarial|workflow-automatizado-n8n)/(.*).html",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}

